# Cloudflare Environment Configuration
# Copy this file to .env.local and customize for your deployment

# Cloudflare Pages Project Name
VITE_PROJECT_NAME=momfood-cloud-kitchen

# App Configuration
VITE_APP_NAME=MomFood
VITE_APP_DESCRIPTION=Cloud Kitchen Platform for Saudi Market

# Development Settings
VITE_DEV_MODE=false

# API Configuration (if you plan to add backend later)
# VITE_API_URL=https://api.your-domain.com
# VITE_API_VERSION=v1

# Analytics (if you plan to add analytics)
# VITE_ANALYTICS_ID=your-analytics-id

# Cloudflare Workers (if you add Workers later)
# VITE_WORKER_URL=https://your-project.your-subdomain.workers.dev